<tr id="<%= dom_id(clinic) %>">
    <td><%= clinic.name %></td>
    <td><%= clinic.address %></td>
    <td><%= clinic.city %></td>
    <td><%= link_to 'Oficinas', clinic_offices_path(clinic), remote: true, target: '_top', 'data-turbo-frame': 'offices' %></td>
    <td><%= link_to 'Editar', edit_clinic_path(clinic), remote: true, target: '_top', 'data-turbo-frame': 'clinics' %></td>
    <td> <%= link_to 'Eliminar', delete_by_get_clinic_path(clinic), method: :get, remote: true, 'data-turbo-frame': 'clinics' %></td>
</tr>